MAX_STATE = [4,4];

pWin = zeros(MAX_STATE) -1;
sArmy = zeros(MAX_STATE) -1;
sDef = zeros(MAX_STATE) -1;
A = zeros(MAX_STATE) -1;
B = zeros(MAX_STATE) -1;

for a = 2:MAX_STATE(1)
    disp(a);
    for b = 1:MAX_STATE(2)-1
        [pWin, sArmy, sDef] = probWin(a, b, pWin, sArmy, sDef);
        disp(b);
    end
end

state_values = pWin;
disp(state_values);
disp(sArmy);
disp(sDef);